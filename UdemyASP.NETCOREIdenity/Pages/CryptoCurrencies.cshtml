@page
@model UdemyASP.NETCOREIdenity.Pages.CryptoCurrenciesModel
@{
}


<table class="table table-bordered table-stripped">  
    <thead>
        <tr>
            <td>Date</td>
            <td>Temperature in C</td>
            <td>Temperature F</td>
            <td>Summary</td>
            <td>Date</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.CryptoCurrencyLiveChangeDTOs)
        {
            <tr>
                <td>@item.Coin</td>
                <td>@item.Price</td>
                <td>@item.MarketCap</td>
                <td>@item.ChangeIn1Hour</td>
                <td>@item.DateTime</td>
            </tr>
        }
    </tbody>
</table>